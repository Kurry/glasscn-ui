{
  "$schema": "https://raw.githubusercontent.com/conventional-changelog/conventional-changelog-config-spec/master/versions/2.2.0/schema.json",
  "types": [
    {
      "type": "feat",
      "section": "Features"
    },
    {
      "type": "fix",
      "section": "Bug Fixes"
    },
    {
      "type": "docs",
      "section": "Documentation"
    },
    {
      "type": "refactor",
      "section": "Refactorings"
    },
    {
      "type": "perf",
      "section": "Optimizations"
    },
    {
      "type": "test",
      "section": "Testing"
    },
    {
      "type": "chore",
      "section": "Other"
    },
    {
      "type": "style",
      "section": "Other"
    }
  ]
}
